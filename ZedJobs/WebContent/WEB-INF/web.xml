<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ZedJobs</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>handlesubmit</servlet-name>
    <servlet-class>com.zedjobs.HandleSubmit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>handlesubmit</servlet-name>
    <url-pattern>/handlesubmit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>com.zedjobs.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getjobs</servlet-name>
    <servlet-class>com.zedjobs.GetJobs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getjobs</servlet-name>
    <url-pattern>/getjobs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signin</servlet-name>
    <servlet-class>com.zedjobs.SignIn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signin</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getuser</servlet-name>
    <servlet-class>com.zedjobs.GetUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getuser</servlet-name>
    <url-pattern>/getuser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>newjob</servlet-name>
    <servlet-class>com.zedjobs.NewJob</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>newjob</servlet-name>
    <url-pattern>/newjob</url-pattern>
  </servlet-mapping>
</web-app>